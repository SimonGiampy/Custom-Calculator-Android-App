<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/apple_symbols" />
            <stroke android:width="1px" android:color="@color/apple_black" />
        </shape>
    </item>
    <item android:drawable="@drawable/backspace_black" android:gravity="center" />
    <item>
        <ripple android:color="?android:attr/colorControlHighlight">
            <item android:id="@android:id/mask" android:drawable="@color/apple_symbols"/>
        </ripple>
    </item>
</layer-list>